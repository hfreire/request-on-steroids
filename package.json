{
  "name": "request-on-steroids",
  "description": "An HTTP client with rate limiter, retry, circuit-breaker and tor support out-of-the-box",
  "version": "0.0.0",
  "engines": {
    "node": ">= 6.0.0"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hfreire/request-on-steroids.git"
  },
  "author": "Hugo Freire <hugo@exec.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hfreire/request-on-steroids/issues"
  },
  "homepage": "https://github.com/hfreire/request-on-steroids#readme",
  "dependencies": {
    "bluebird": "3.5.5",
    "lodash": "4.17.15",
    "perseverance": "1.0.33",
    "random-http-useragent": "1.1.17",
    "request": "2.88.0",
    "socks5-http-client": "1.0.4",
    "socks5-https-client": "1.2.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "3.0.5",
    "eslint": "6.7.0",
    "eslint-config-hfreire": "1.1.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-jest": "22.14.0",
    "eslint-plugin-mocha": "6.0.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "10.0.0",
    "istanbul": "0.4.5",
    "mocha": "6.2.0",
    "pre-git": "3.17.1",
    "semantic-release": "15.13.18",
    "snyk": "2.0.0",
    "testdouble": "3.12.2"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "allow-untracked-files": true
    }
  },
  "scripts": {
    "clean": "rm -rf lib coverage",
    "snyk:test": "./node_modules/.bin/snyk test",
    "snyk:protect": "./node_modules/.bin/snyk protect",
    "lint": "./node_modules/.bin/eslint . --ext .json --ext .js",
    "coverage": "./node_modules/.bin/istanbul cover --include-all-sources --root src --print detail ./node_modules/mocha/bin/_mocha -- --recursive test",
    "prepare": "npm run snyk:protect",
    "test": "npm run lint && npm run coverage",
    "compile": "npm run clean && mkdir -p lib && ./node_modules/.bin/babel src/ -d lib",
    "prepublish": "npm run compile",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "semantic-release": "./node_modules/.bin/semantic-release",
    "commit": "./node_modules/.bin/commit-wizard"
  },
  "snyk": true
}
